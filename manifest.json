   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "38": "icons/38.png",
      "48": "icons/48.png"
   },
   "manifest_version": 2,
   "name": "Get From Maani Pop-up",
   "options_page": "options.html",
