{
   "background": {
      "persistent": false,
      "scripts": [ "injection.js", "main_compiled.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icons/16.png",
         "38": "icons/38.png"
      },
      "default_title": "Arabic Maani"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "bubble_gss.css" ],
      "js": [ "bubble_compiled.js" ],
